{
  "schemaVersion": 1,
  "id": "tpshud",
  "version": "${version}",
  "name": "TPS Hud",
  "description": "A mod for fabric that displays the servers tps.",
  "authors": [
    "mooziii"
  ],
  "contact": {
    "sources": "https://github.com/mooziii/tpshud-fabric",
    "issues": "https://github.com/mooziii/tpshud-fabric/issues",
    "homepage": "https://github.com/mooziii/tpshud-fabric"
  },
  "license": "MIT",
  "icon": "assets/tpshud/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "me.obsilabor.tpshud.TpsHud"
    ],
    "modmenu": [
      "me.obsilabor.tpshud.ModMenuApiImpl"
    ],
    "server": [
      "me.obsilabor.tpshud.server.TpsHudFabricServer"
    ]
  },
  "mixins": [
    "tpshud.mixins.json"
  ],
  "depends": {
    "fabric-api": "*",
    "fabric-language-kotlin": "*",
    "minecraft": ">=1.21.4"
  },
  "recommends": {
    "modmenu": "*",
    "yet_another_config_lib_v3": "*"
  }
}
